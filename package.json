{
  "name": "mp3-mediarecorder",
  "version": "0.0.1",
  "description": "MediaRecorder ponyfill that records audio as mp3",
  "module": "dist/mp3-mediarecorder.esm.js",
  "browser": "dist/mp3-mediarecorder.umd.js",
  "scripts": {
    "test": "jest",
    "format": "npm run format:write",
    "format:write": "prettier --write **/*.{ts,js,yml,json,md}",
    "format:check": "prettier -l **/*.{ts,js,yml,json,md}",
    "tsc": "tsc --noEmit",
    "build": "rollup -c",
    "build:watch": "rollup -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliasmeire/mp3-mediarecorder.git"
  },
  "keywords": [
    "mediarecorder",
    "polyfill",
    "ponyfill",
    "mp3",
    "mediastream",
    "audio",
    "encoding",
    "libmp3lame",
    "webassembly",
    "vmsg"
  ],
  "author": "Elias Meire",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eliasmeire/mp3-mediarecorder/issues"
  },
  "homepage": "https://github.com/eliasmeire/mp3-mediarecorder#readme",
  "devDependencies": {
    "@types/dom-mediacapture-record": "^1.0.0",
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "rollup": "^0.67.4",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-wasm": "^3.0.0",
    "semantic-release": "^15.12.5",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "vmsg": "^0.3.5"
  }
}
